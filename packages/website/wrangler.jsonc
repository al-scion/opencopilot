{
  "name": "opencopilot-website",
  "compatibility_date": "2025-12-30",
  "compatibility_flags": ["nodejs_compat"],
  "preview_urls": true,
  "workers_dev": true,
  "main": "./dist/_worker.js/index.js",
  "routes": [
    { "pattern": "www.usefabric.xyz", "custom_domain": true },
    { "pattern": "usefabric.xyz", "custom_domain": true }
